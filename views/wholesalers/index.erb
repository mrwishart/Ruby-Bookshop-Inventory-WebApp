<div class = "sub-header-container">
  <div class = "sub-header">
    <a href="/wholesalers/new">Add Wholesaler</a>
  </div>
</div>

<div class="info-table">

<table>
  <tr>
    <th>Name</th>
    <th>Discount Offered</th>
    <th>Address</th>
    <th>Contact Number</th>
  </tr>

  <% for wholesaler in @wholesalers %>
  <tr>
    <td><a href="/wholesalers/<%=wholesaler.id%>"><%=wholesaler.name%></a></td>
    <td><%=wholesaler.discount_offered%>%</td>
    <td><%=wholesaler.address%></td>
    <td><%=wholesaler.contact_number%></td>
  </tr>

  <% end %>
</table>
</div>
