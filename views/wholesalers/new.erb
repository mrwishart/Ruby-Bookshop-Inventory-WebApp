
<form action="/wholesalers" method="post">
<div class = "sub-header-container">
  <div class = "sub-header">
    <div class="button">
      <input type ="submit" value="Create Wholesaler">
    </div>
  </div>
</div>

<div class="info-table">

  <table>
    <tr>
      <td><label for="name">Name:</label></td>
      <td><input id="name" type="text" name="name"></td>
    </tr>
    <tr>
      <td><label for="address">Address:</label></td>
      <td><input id="address" type="text" name="address"></td>
    </tr>
    <tr>
      <td><label for="contact-number">Contact Number:</label></td>
      <td><input id="contact-number" type="text" name="contact_number"></td>
    </tr>
    <tr>
      <td><label for="discount-offered">Discount Offered:</label></td>
      <td><input id="discount-offered" type="number" min="0" max="100" step = "0.01" name="discount_offered">%</td>
    </tr>
  </table>

  </div>


</form>
